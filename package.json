{
  "name": "sensor-interactive",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "tslint --project .",
    "copy-public": "ncp src/public/ dist/",
    "copy-public:watch": "onchange \"src/public/**/*.*\" -v -- npm run copy-public",
    "webpack": "cross-env NODE_ENV=production webpack --mode production",
    "webpack:watch": "cross-env NODE_ENV=production webpack -w --mode production",
    "dev-webpack": "cross-env NODE_ENV=development webpack --mode development",
    "dev-webpack:watch": "cross-env NODE_ENV=development webpack -w --mode development",
    "browse": "live-server dist/",
    "build": "npm-run-all --parallel copy-public webpack",
    "dev-build": "npm-run-all --parallel copy-public dev-webpack",
    "start": "npm-run-all --parallel copy-public copy-public:watch webpack:watch browse",
    "dev-start": "npm-run-all --parallel copy-public copy-public:watch dev-webpack:watch browse",
    "deploy": "./build_and_deploy.sh",
    "cloudfront:invalidations": "s3cmd cfinvalinfo cf://E3V22UD5MXF4CR",
    "cloudfront:inval-root": "aws cloudfront create-invalidation --distribution-id E3V22UD5MXF4CR --paths /index.html",
    "test:full": "npm-run-all test test:cypress",
    "test:cypress": "cypress run --config video=false,defaultCommandTimeout=10000 --env testEnv=local",
    "test:cypress:ci": "cypress run --config video=false,defaultCommandTimeout=10000 --env testEnv=local",
    "test:cypress:smoke": "cypress run --spec 'cypress/integration/smoke/*' --config video=false,defaultCommandTimeout=10000 --env testEnv=local",
    "test:cypress:branch": "cypress run --spec 'cypress/integration/branch/**/*' --config video=false,defaultCommandTimeout=10000 --env testEnv=local",
    "test:cypress:staging": "cypress run --config video=false,defaultCommandTimeout=10000 --env testEnv=staging,failOnSnapshotDiff=false",
    "test:production": "cypress open --env testEnv=production,failOnSnapshotDiff=false",
    "test:staging": "cypress open --env testEnv=staging,failOnSnapshotDiff=false",
    "test:branch": "cypress open --env testEnv=branch,failOnSnapshotDiff=false",
    "test:local": "cypress open --env testEnv=local,failOnSnapshotDiff=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/sensor-interactive.git"
  },
  "author": "Concord Consortium",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/concord-consortium/sensor-interactive/issues"
  },
  "homepage": "https://github.com/concord-consortium/sensor-interactive#readme",
  "dependencies": {
    "@concord-consortium/sensor-connector-interface": "^0.1.4",
    "@vernier/godirect": "1.4.3",
    "d3-format": "^1.4.3",
    "dygraphs": "^2.1.0",
    "iframe-phone": "^1.2.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-sizeme": "^2.6.12"
  },
  "devDependencies": {
    "@types/d3-format": "^1.3.1",
    "@types/dygraphs": "^1.1.10",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-modal": "^3.10.5",
    "@types/web-bluetooth": "0.0.4",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^5.2.1",
    "cypress": "^4.2.0",
    "cypress-commands": "^1.0.0",
    "cypress-image-snapshot": "^3.1.1",
    "live-server": "^1.2.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
